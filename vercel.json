{
  "builds": [
    {
      "src": "Backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/main.py",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}