{
  "buildCommand": "pip install -r Backend/requirements.txt",
  "outputDirectory": ".",
  "trailingSlash": false,
  "env": {
    "ENVIRONMENT": "production",
    "PYTHONUNBUFFERED": "1"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    },
    {
      "src": "/api$",
      "dest": "/api/index.py",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}